{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "resolveJsonModule": true,
    "paths": {
      "@firebase/auth": ["./node_modules/@firebase/auth/dist/index.rn.d.ts"],
      "@assets/*": ["./assets/*"],
      "@components/*": ["./components/*"],
      "@config/*": ["./config/*"],
      "@hooks/*": ["./hooks/*"],
      "@routes": ["./routes"],
      "@routes/*": ["./routes/*"],
      "@screens/*": ["./screens/*"],
      "@screens": ["./screens"],
      "@store/*": ["./store/*"],
      "@utils/*": ["./utils/*"],
      "src/types/*": ["./types/*"]
    }
  }
}
